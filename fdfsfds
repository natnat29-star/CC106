// Variables
let name = "Mark";
let age = 22;
let gender = "Male";
let isStudent = true;
console.log(`Name: ${name}, Age: ${age}, Gender: ${gender}, Student: ${isStudent}`);

for (let i = 1; i <= 10; i++) {
 console.log(i);
}


function addNumbers(a, b) {
 return a + b;
}
console.log(addNumbers(5, 10));


if (age >= 22) {
 console.log("Adult");
} else {
 console.log("Minor");
}
